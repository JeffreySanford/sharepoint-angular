{
  "name": "spfx-angular-uptime",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=16.0.0"
  },
  "overrides": {
    "node-sass": "npm:sass@^1.69.0"
  },
  "scripts": {
    "build": "npx gulp bundle",
    "clean": "npx gulp clean",
    "test": "npx gulp test",
    "build:angular": "cd src/webparts/uptimeStatus/angularApp && npm run build",
    "start:api": "ts-node server/src/main.ts",
    "start:spfx:legacy": "npx gulp serve --nobrowser",
    "start:spfx:dev": "webpack serve --config webpack.dev.js",
    "build:spfx:dev": "webpack --config webpack.dev.js",
    "start:dev": "concurrently \"npm run start:api\" \"npm run start:spfx:dev\"",
    "start": "concurrently \"npm run start:api\" \"npm run start:spfx:dev\""
  },
  "dependencies": {
    "@azure/msal-angular": "^4.0.12",
    "@azure/msal-browser": "^4.13.2",
    "@microsoft/sp-core-library": "1.11.0",
    "@microsoft/sp-lodash-subset": "1.11.0",
    "@microsoft/sp-office-ui-fabric-core": "1.11.0",
    "@microsoft/sp-property-pane": "1.11.0",
    "@microsoft/sp-webpart-base": "1.11.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^11.1.3",
    "@nestjs/platform-express": "^11.1.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0"
  },
  "devDependencies": {
    "@microsoft/rush-stack-compiler-3.3": "0.3.5",
    "@microsoft/sp-build-web": "1.11.0",
    "@microsoft/sp-module-interfaces": "1.11.0",
    "@microsoft/sp-tslint-rules": "1.11.0",
    "@microsoft/sp-webpart-workbench": "1.11.0",
    "@types/node": "^18.0.0",
    "concurrently": "^8.0.0",
    "css-loader": "^6.8.0",
    "gulp": "~3.9.1",
    "gulp-cli": "^2.3.0",
    "html-webpack-plugin": "^5.5.0",
    "sass": "^1.69.0",
    "sass-loader": "^13.3.0",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.4.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.9.0",
    "webpack": "^5.80.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0"
  }
}

{
  "name": "spfx-angular-uptime",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=18.0.0 <19.0.0"
  },
  "overrides": {
    "node-sass": "npm:sass@^1.69.0"
  },
  "scripts": {
    "build": "node -r ./node-compat.js ./node_modules/gulp/bin/gulp.js bundle",
    "clean": "node -r ./node-compat.js ./node_modules/gulp/bin/gulp.js clean",
    "test": "node -r ./node-compat.js ./node_modules/gulp/bin/gulp.js test",
    "build:angular": "cd src/webparts/uptimeStatus/angularApp && npm run build",
    "build:angular:update": "npm run build:angular && node scripts/update-angular-assets.js",
    "update-workbench": "node scripts/update-workbench-assets.js",
    "build:full": "npm run build:angular && npm run update-workbench",
  "start:api": "ts-node server/src/main.ts",
  "start:spfx": "webpack serve --config webpack.dev.js",
  "start:spfx:dev": "npm run start:spfx",
    "start:angular": "cd src/webparts/uptimeStatus/angularApp && npm start",
    "build:spfx:dev": "webpack --config webpack.dev.js",
    "start:dev": "concurrently \"npm run start:api\" \"npm run start:spfx:dev\" \"npm run start:angular\"",
    "start:all:dev": "concurrently \"npm run start:backend\" \"npm run start:spfx:dev\" \"npm run start:frontend\"",
    "start": "npm run start:all:dev",
    "update-assets": "node scripts/update-angular-assets.js",
  "start:backend": "npx ts-node server/src/main.ts",
    "start:frontend": "npm run start:angular",
    "start:all": "concurrently \"npm run start:backend\" \"npm run start:frontend\""
  },
  "dependencies": {
    "@aws-crypto/sha256-browser": "^5.2.0",
    "@azure/msal-angular": "^2.0.0",
    "@azure/msal-browser": "^2.0.0",
    "@microsoft/sp-core-library": "1.16.1",
    "@microsoft/sp-lodash-subset": "1.16.1",
    "@microsoft/sp-office-ui-fabric-core": "1.16.1",
    "@microsoft/sp-property-pane": "1.16.1",
    "@microsoft/sp-webpart-base": "1.16.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mongoose": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.0.0",
    "@nestjs/throttler": "^5.0.0",
    "@types/express-rate-limit": "^5.1.3",
    "@types/multer": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "express-rate-limit": "^6.7.0",
    "mongoose": "^6.13.0",
    "multer": "^1.4.5-lts.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@microsoft/rush-stack-compiler-3.3": "0.3.5",
    "@microsoft/sp-build-web": "1.21.1",
    "@microsoft/sp-module-interfaces": "1.16.1",
    "@microsoft/sp-tslint-rules": "1.16.1",
    "@types/es6-promise": "^3.3.2",
    "@types/minimatch": "^6.0.0",
    "@types/node": "^18.0.0",
    "@types/retry": "^0.12.5",
    "@types/webpack-env": "^1.18.8",
    "concurrently": "^7.6.0",
    "css-loader": "^6.7.3",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "html-webpack-plugin": "^5.5.0",
    "mongodb-memory-server": "^8.11.0",
    "nx": "21.2.1",
    "path-browserify": "^1.0.1",
    "sass": "^1.62.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.5",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  }
}

{
  "name": "spfx-angular-uptime",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=16.0.0"
  },
  "overrides": {
    "node-sass": "npm:sass@^1.69.0"
  },
  "scripts": {
    "build": "node -r ./node-compat.js ./node_modules/gulp/bin/gulp.js bundle",
    "clean": "node -r ./node-compat.js ./node_modules/gulp/bin/gulp.js clean",
    "test": "node -r ./node-compat.js ./node_modules/gulp/bin/gulp.js test",
    "build:angular": "cd src/webparts/uptimeStatus/angularApp && npm run build",
    "build:angular:update": "npm run build:angular && node scripts/update-angular-assets.js",
    "update-workbench": "node scripts/update-workbench-assets.js",
    "build:full": "npm run build:angular && npm run update-workbench",
    "start:api": "ts-node server/src/main.ts",
    "start:spfx": "webpack serve --config webpack.dev.js",
    "start:angular": "cd src/webparts/uptimeStatus/angularApp && npm start",
    "build:spfx:dev": "webpack --config webpack.dev.js",
    "start:dev": "concurrently \"npm run start:api\" \"npm run start:spfx:dev\" \"npm run start:angular\"",
    "start": "concurrently \"npm run start:api\" \"npm run start:angular\" \"npm run start:spfx\"",
    "update-assets": "node scripts/update-angular-assets.js"
  },
  "dependencies": {
    "@azure/msal-angular": "^4.0.12",
    "@azure/msal-browser": "^4.13.2",
    "@microsoft/sp-core-library": "1.11.0",
    "@microsoft/sp-lodash-subset": "1.11.0",
    "@microsoft/sp-office-ui-fabric-core": "1.11.0",
    "@microsoft/sp-property-pane": "1.11.0",
    "@microsoft/sp-webpart-base": "^1.21.1",
    "@nestjs/common": "^11.1.5",
    "@nestjs/core": "^11.1.5",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/platform-express": "^11.1.5",
    "@nestjs/swagger": "^11.2.0",
    "@types/multer": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "mongoose": "^7.8.7",
    "multer": "^2.0.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0"
  },
  "devDependencies": {
    "@microsoft/rush-stack-compiler-3.3": "^0.11.2",
    "@microsoft/sp-build-web": "1.11.0",
    "@microsoft/sp-module-interfaces": "1.11.0",
    "@microsoft/sp-tslint-rules": "1.11.0",
    "@microsoft/sp-webpart-workbench": "^0.1.12",
    "@types/node": "^18.0.0",
    "concurrently": "^8.0.0",
    "css-loader": "^6.8.0",
    "gulp": "~3.9.1",
    "gulp-cli": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "mongodb-memory-server": "^10.1.4",
    "nx": "21.2.1",
    "path-browserify": "^1.0.1",
    "sass": "^1.69.0",
    "sass-loader": "^13.3.0",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.4.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.9.0",
    "webpack": "^5.80.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^5.2.2"
  },
  "nx": {}
}
